<launch>


 <arg name="robot_name" default="/" />

 <param unless="$(eval robot_name=='/')" name="tf_prefix" value="$(arg robot_name)" />
 
  <group ns="$(arg robot_name)">
     <include file="$(find bv80bot_node)/launch/include/bv80bot_map.launch" />
     <include file="$(find bv80bot_node)/launch/include/bv80bot_gui.launch" />
  </group>

</launch>

